{
  "name": "jambagecom/tt-products",
  "type": "typo3-cms-extension",
  "description": "Shop system extension for TYPO3 CMS",
  "keywords": [
    "TYPO3",
    "extension",
    "shop",
    "basket",
    "cart"
  ],
  "support": {
    "issues": "https://jambage.com/kontakt/forum.html"
  },
  "homepage": "https://jambage.com",
  "authors": [
    {
      "name": "Franz Holzinger",
      "role": "Developer",
      "homepage": "https://ttproducts.de"
    }
  ],
  "license": [
    "GPL-2.0-or-later"
  ],
  "require": {
    "php": "^5.5",
    "typo3/cms-core": ">=6.2.0,<=9.5.99",
    "jambagecom/div2007": ">=1.10.30,<2",
    "jambagecom/table": ">=0.7.0,<1",
    "jambagecom/tsparser": ">=0.2.5,<1"
  },
  "suggest": {
    "jambagecom/func-wizards": ">=0.0.1",
    "jambagecom/addons-em": ">=0.2.1",
    "friendsoftypo3/typo3db-legacy": ">=1.0.0,<1.99"
  },
  "autoload": {
    "psr-4": {
      "JambageCom\\TtProducts\\": "Classes"
    },
    "classmap": [
      "api",
      "Classes",
      "control",
      "eid",
      "hooks",
      "lib",
      "marker",
      "model",
      "modfunc1",
      "modfunc2",
      "pi1",
      "pi_int",
      "pi_search",
      "view"
    ]
  },
  "replace": {
    "jambagecom/tt-products": "self.version",
    "typo3-ter/tt-products": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "tt_products"
    }
  }
}
